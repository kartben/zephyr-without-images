if SOC_STM32G071XX

config SOC
	default "stm32g071xx"

config NUM_IRQS
	default 32

config CLOCK_STM32_PLL_Q_DIVISOR
	int "PLL Q Divisor"
	depends on CLOCK_STM32_SYSCLK_SRC_PLL
	default 2
	range 2 8
	help
	  PLL Q VCO divisor, allowed values: 2-8

endif # SOC_STM32G071XX
