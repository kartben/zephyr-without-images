		spi0: spi@40610000 {
			compatible = "infineon,cat1-spi";
			reg = <0x40610000 0x10000>;
			#address-cells = <1>;
			#size-cells = <0>;
			interrupts = <41 6>;
			status = "disabled";
		};
		spi1: spi@40620000 {
			compatible = "infineon,cat1-spi";
			reg = <0x40620000 0x10000>;
			#address-cells = <1>;
			#size-cells = <0>;
			interrupts = <42 6>;
