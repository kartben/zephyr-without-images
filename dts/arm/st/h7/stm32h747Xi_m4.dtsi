#include <mem.h>
#include <st/h7/stm32h747.dtsi>

/delete-node/ &flash0;

/ {
	cpus {
		/delete-node/ cpu@0;
	};

	soc {
		flash-controller@52002000 {
			flash1: flash@8100000 {
				label = "FLASH_STM32";
				reg = <0x08100000 DT_SIZE_K(1024)>;
			};
		};
	};
};
