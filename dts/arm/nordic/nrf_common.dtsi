#include <dt-bindings/i2c/i2c.h>
#include <dt-bindings/gpio/gpio.h>

&systick {
	/*
	 * Nordic SoCs rely by default on the RTC for system clock
	 * implementation, so the SysTick node is not to be enabled.
	 */
	status = "disabled";
};
