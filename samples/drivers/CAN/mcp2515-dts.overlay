&spi1 {
	status = "ok";
	mcp2515@0 {
		compatible = "microchip,mcp2515";
		spi-port-name = "SPI_1";
		spi-max-frequency = <1000000>;
		int-gpios = <&gpiob 4 GPIO_INT_ACTIVE_LOW>;
		status = "ok";
		label = "CAN_1";
		reg = <0>;
	};
};
