config SOC_FAMILY_RISCV_PRIVILEGE
	bool
	# omit prompt to signify a "hidden" option
	default n

config SOC_FAMILY
	string
	default "riscv-privilege"
	depends on SOC_FAMILY_RISCV_PRIVILEGE

source "arch/riscv32/soc/riscv-privilege/*/Kconfig.defconfig.series"
