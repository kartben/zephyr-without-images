/dts-v1/;

#include <nxp/nxp_mimx93_a55.dtsi>

/ {
	model = "NXP i.MX93 A55";
	compatible = "fsl,mimx93";

	chosen {
		zephyr,console = &lpuart2;
		zephyr,shell-uart = &lpuart2;
		zephyr,sram = &sram0;
	};

	cpus {
		cpu@0 {
			status = "disabled";
		};
	};

	sram0: memory@c0000000 {
		reg = <0xc0000000 DT_SIZE_M(1)>;
	};
};

&lpuart2 {
	status = "okay";
	current-speed = <115200>;
};
