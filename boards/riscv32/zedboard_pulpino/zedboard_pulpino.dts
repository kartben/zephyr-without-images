/dts-v1/;

#include <pulpino.dtsi>

/ {
	model = "zedboard_pulpino";
	compatible = "pulp,zedboard-pulpino";

	aliases {
		uart-0 = &uart0;
		gpio-0 = &gpio0;
	};

	chosen {
		zephyr,console = &uart0;
	};
};

&uart0 {
	status = "ok";
	current-speed = <115200>;
};

&gpio0 {
	status = "ok";
};
